<div>
  <% page_link.page_links.each do |link| %>
		<ul>
      <li>
        <% if link.submenue %>
          <%= link.name.upcase %>
          <%= render 'layouts/sub_layouts/sub_layout', page_link: link %>
        <% else %>
          <%= compose_path(link) %>
        <% end %>
      </li>
    </ul>
  <% end %>
  <%= link_to "Add link", "#{request.base_url}/#{@scope}/page_links/new?page_link_id=#{page_link.id}" %>
</div>